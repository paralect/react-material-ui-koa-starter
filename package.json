{
  "name": "react-material-ui-koa-starter",
  "version": "1.0.0",
  "description": "React starter with Material design components",
  "private": true,
  "author": "",
  "license": "MIT",
  "repository": "git@github.com:paralect/react-material-ui-koa-starter.git",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "nwb build-react-app",
    "clean": "nwb clean-app",
    "start": "nwb serve-react-app",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/data-grid": "^4.0.0-alpha.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/system": "^4.9.14",
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "dotenv-webpack": "^3.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "formik": "^2.1.5",
    "formik-material-ui": "^3.0.0",
    "formik-material-ui-pickers": "0.0.11",
    "lodash": "^4.17.20",
    "notistack": "^1.0.0",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "^2.23.0",
    "react-query-devtools": "^2.5.0",
    "react-router-dom": "^5.2.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "nwb": "0.25.x"
  }
}
