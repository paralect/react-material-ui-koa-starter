{
  "name": "react-material-ui-koa-starter",
  "version": "1.0.0",
  "description": "React starter with Material design components",
  "private": true,
  "license": "MIT",
  "repository": "git@github.com:paralect/react-material-ui-koa-starter.git",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "vercel-build": "razzle build",
    "vercel-start": "NODE_ENV=production node build/server.js",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "contributors:check": "all-contributors check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^2.10.6",
    "@loadable/component": "^5.13.2",
    "@material-ui/core": "^4.11.0",
    "@material-ui/data-grid": "^4.0.0-alpha.6",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/system": "^4.9.14",
    "axios": "^0.21.3",
    "date-fns": "^2.16.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "formik": "^2.1.5",
    "formik-material-ui": "^3.0.0",
    "formik-material-ui-pickers": "0.0.11",
    "lodash": "^4.17.20",
    "notistack": "^1.0.0",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "razzle-plugin-svg-react-component": "^1.0.3",
    "razzle-plugin-workbox": "^1.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "^2.23.0",
    "react-query-devtools": "^2.5.1",
    "react-router-dom": "^5.2.0",
    "shrink-ray-current": "^4.1.2",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.0.0",
    "all-contributors-cli": "^6.18.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "razzle": "^3.3.0"
  },
  "optionalDependencies": {
    "iltorb": "^2.4.5",
    "node-zopfli-es": "^1.0.6"
  }
}
